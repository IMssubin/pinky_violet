<?xml version='1.0' ?>

<launch>
    <arg name="fiducial_len" default="0.07"/>
    <arg name="read_only" default="true"/>
    <arg name="publish_tf" default="false"/>

    <include file="$(find-pkg-share aruco_detect)/launch/aruco_detect.launch.py">
        <arg name="fiducial_len" value="$(var fiducial_len)"/>
    </include>

    <include file="$(find-pkg-share fiducial_slam)/fiducial_slam.launch.py">
        <arg name="read_only_map" value="$(var read_only)"/>
        <arg name="publish_tf" value="$(var publish_tf)"/>

    </include>
</launch>